{
    "name": "decodelabs/df-base",
    "description": "Base DF package",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Tom Wright",
        "email": "tom@inflatablecookie.com"
    }],
    "require": {
        "php": "^7.2",
        "symfony/polyfill-mbstring": "~1.7",

        "decodelabs/exceptional": "^0.2",
        "decodelabs/glitch": "^0.16.2",
        "decodelabs/veneer": "^0.5.5",
        "decodelabs/systemic": "^0.6.9",
        "decodelabs/tagged": "^0.8.4",
        "decodelabs/terminus": "^0.5.6",
        "decodelabs/collections": "^0.5.6",
        "decodelabs/gadgets": "^0.6.2",
        "decodelabs/atlas": "^0.7.6",

        "psr/container": "^1.0",
        "psr/cache": "^1.0",
        "psr/simple-cache": "^1.0",
        "psr/log": "^1.0",
        "psr/link": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/http-server-handler": "^1.0",

        "monolog/monolog": "~1.23",
        "ramsey/uuid": "^3.7",
        "moontoast/math": "^1.1",
        "paragonie/halite": "^4",
        "paragonie/sodium_compat": "^1",
        "nesbot/carbon": "^2"
    },
    "require-dev": {
        "phpstan/phpstan": "0.12.45",
        "phpstan/extension-installer": "^1.0",
        "decodelabs/phpstan-decodelabs": "dev-develop",

        "predis/predis": "^1.1"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Df\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
