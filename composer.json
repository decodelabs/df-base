{
    "name": "decodelabs/df-base",
    "description": "Base DF package",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/polyfill-mbstring": "~1.7",

        "decodelabs/glitch": "<2",
        "decodelabs/gadgets": "~0.6",
        "decodelabs/collections": "~0.5",
        "decodelabs/tagged": "~0.7",
        "decodelabs/veneer": "~0.5",
        "decodelabs/systemic": "~0.6",
        "decodelabs/atlas": "~0.7",
        "decodelabs/terminus": "dev-develop",

        "psr/container": "^1.0",
        "psr/cache": "^1.0",
        "psr/simple-cache": "^1.0",
        "psr/log": "^1.0",
        "psr/link": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/http-server-handler": "^1.0",

        "monolog/monolog": "~1.23",
        "ramsey/uuid": "^3.7",
        "moontoast/math": "^1.1",
        "paragonie/halite": "^4",
        "paragonie/sodium_compat": "^1",
        "nesbot/carbon": "^2"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11.16",
        "phpstan/extension-installer": "^1.0",
        "decodelabs/phpstan-decodelabs": "^0.2",

        "decodelabs/glitch": "dev-develop",
        "decodelabs/veneer": "dev-develop",
        "decodelabs/systemic": "dev-develop",
        "decodelabs/tagged": "dev-develop",
        "decodelabs/terminus": "dev-develop",
        "decodelabs/collections": "dev-develop",
        "decodelabs/gadgets": "dev-develop",
        "decodelabs/atlas": "dev-develop",
        "decodelabs/enlighten": "dev-develop",

        "predis/predis": "^1.1"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Df\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
